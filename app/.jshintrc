{
  "node": true,
  "browser": true,
  "esnext": true,
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "immed": true,
  "indent": 4,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "regexp": true,
  "undef": true,
  "unused": true,
  "strict": true,
  "validthis": true, //turn this on to surpress warning while defining a provider / service function
  "trailing": true,
  "smarttabs": true,
  "latedef": false,
  "globals": {
    "angular": false,
    "$": false,
    "describe": true,
    "it": true,
    "expect": true,
    "inject": true,
    "$controller": true,
    "beforeEach": true
  }
}
